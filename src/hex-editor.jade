.hex(ng-controller='HexEditor.HexEditorCtrl')
	button.btn.btn-primary(ng-click="curRow = curRow + cols") Next Row
	table.hex.table.table-bordered.table-hover
		thead
			tr
				td Address
				td(ng-repeat="col in range(cols)") {{hex(col, 2)}}
		tbody
			tr(ng-repeat="row in range(rows)")
				td {{hex(curRow+row*cols)}}
				td(ng-repeat="col in range(cols)") {{getByte(row, col)}}